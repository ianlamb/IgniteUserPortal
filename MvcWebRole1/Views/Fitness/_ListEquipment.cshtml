@model IEnumerable<Ignite.Models.EquipmentListViewModel>

@if (Model.Count() > 0) {
    foreach (var e in Model) {
        <div class="row equipment" id="@e.Name">
            <img src="@(e.Thumbnail != null ? e.Thumbnail : "../../Content/images/equipment/exercise-thumb.jpg")" alt="equipment thumbnail" />
        </div> 
    }
}
else
{
    <div>No results.</div>
}